{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/api/index.js"
      },
      {
        "source": "/(.*)",
        "destination": "/"
      }
    ],
    "functions": {
      "api/index.js": {
        "memory": 1024,
        "maxDuration": 60
      }
    }
  }